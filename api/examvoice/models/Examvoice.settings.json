{
  "connection": "default",
  "collectionName": "examvoice",
  "info": {
    "name": "examVoice",
    "description": "Singola voce da valutare, per un totale di 22 per ogni esame"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "Nome": {
      "type": "string",
      "required": true
    },
    "Valutazione": {
      "max": 2,
      "type": "integer"
    },
    "category": {
      "model": "category",
      "via": "examVoices"
    },
    "exam": {
      "model": "exam",
      "via": "examVoices"
    }
  }
}